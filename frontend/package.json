    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index --again",
      "pre-push": "node handleTestCoverage.js"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint"
      ]
    }
